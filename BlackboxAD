blackbox*frameColor:              darkgrey
blackbox*frameTexture:            Solid
blackbox*focusColor:              darkslategrey
blackbox*unfocusColor:             grey

blackbox*buttonTexture:           Solid
blackbox*buttonColor:             darkgrey

blackbox*iconColor:               black
blackbox*menuTexture:             Solid
blackbox*menuColor:               darkgrey
blackbox*menuItemTexture:         Solid
blackbox*menuItemPressedTexture:  Solid
blackbox*menuItemColor:           darkgrey
! this is also the pressed color
blackbox*menuItemToColor:         darkslategrey
blackbox*workspaces:	          3

